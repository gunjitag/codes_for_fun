is_prime <- function(x) {
smallest_primefac <- if(x %% 2 == 0) 2 else 0
i <- 3
while(smallest_primefac == 0 & i <= sqrt(x)) {
smallest_primefac <- if(x %% i == 0) i else 0
i = i+2
}
output <- if(smallest_primefac == 0) x else if(x == 2) x else 0
